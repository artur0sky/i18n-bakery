{
    "name": "example-react-app",
    "private": true,
    "version": "0.0.0",
    "dependencies": {
        "@i18n-bakery/core": "workspace:*",
        "@i18n-bakery/react": "workspace:*",
        "react": "19.2.1",
        "react-dom": "19.2.1"
    },
    "devDependencies": {
        "@i18n-bakery/cli": "workspace:*",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.1",
        "@vitejs/plugin-react": "4.2.1",
        "vite": "5.0.10"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "i18n:extract": "i18n-bakery batter src --locale en --out locales",
        "i18n:bake": "i18n-bakery bake locales --out public/locales --split --manifest manifest.json"
    }
}