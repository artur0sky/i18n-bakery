{
    "name": "@i18n-bakery/poacher",
    "version": "1.0.7",
    "description": "Recovers and migrates translations from other libraries (like i18next) to i18n-bakery architecture. 'Poaching' your legacy recipes for a better taste.",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "bin": {
        "poacher": "./bin/poacher.js"
    },
    "files": [
        "dist",
        "bin"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
        "test": "vitest run"
    },
    "keywords": [
        "i18n",
        "migration",
        "converter",
        "i18next",
        "bakery",
        "poacher"
    ],
    "author": "Arturo SÃ¡enz",
    "license": "MIT",
    "dependencies": {
        "@i18n-bakery/core": "workspace:*",
        "@i18n-bakery/baker": "workspace:*",
        "chalk": "4.1.2",
        "cac": "^6.7.14",
        "fs-extra": "11.2.0"
    },
    "devDependencies": {
        "@types/fs-extra": "11.0.4",
        "@types/node": "20.10.6",
        "tsup": "8.0.1",
        "typescript": "5.3.3",
        "vitest": "1.1.0"
    }
}